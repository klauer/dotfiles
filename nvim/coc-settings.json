{
    "python.jediEnabled": false,
    // Timeout for completion, in milliseconds.,  default: `2000`
    "suggest.timeout": 500,
	// Enable signs for diagnostics,  default: `true`
    "diagnostic.enableSign": true,
	// Highlight the line number, too
    "diagnostic.enableHighlightLineNumber": true,
    // Create location list for error & warning,  default: `true`
    "diagnostic.locationlist": true,
    //  Text of error sign,  default: `">>"`
    "diagnostic.errorSign": "!!",
    //  Text of warning sign,  default: `"⚠"`
    "diagnostic.warningSign": "⚠",
    //  Text of info sign,  default: `">>"`
    "diagnostic.infoSign": ">>",
    //  Text of hint sign,  default: `">>"`
    "diagnostic.hintSign": ">>",
    //  Enable `codeLens` feature, require neovim with set virtual text feature.,  default: `false`
    "codeLens.enable": true,
    //  Separator text for `codeLens` in virtual text,  default: `"‣"`
    "codeLens.separator": "*",
    // Key used for select next line on insert mode.,  default: `"<C-j>"`
    "list.nextKeymap": "<C-n>",
    // Key used for select previous line on insert mode.,  default: `"<C-k>"`
    "list.previousKeymap": "C-p",
    // Max height for preview window of list, default: `12`
    "list.maxPreviewHeight": 20,
    //Use Vim's quickfix list for jump locations. Requires restart on change, default: `false`
    "coc.preferences.useQuickfixForLocations": true,
    // Patterns to resolve workspaceFolder from parent folders of opened files, resolved from up to down.,  default
    "coc.preferences.rootPatterns": [".vim",".git",".hg",".projections.json"],
    // Function string that current cursor in.  in coc-settings.json to update it on CursorHold.
    // TODO: not working?
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    // try integration with ALE
    "coc.preferences.diagnostic.displayByAle": false,
    "list.normalMappings": {
        "<C-c>": "do:exit"
     },
     "list.returns": {
        "<C-c>": "do:exit"
     },
     "list.insertMappings": {
        "<C-c>": "do:exit"
     },
     "suggest.floatEnable": true,
     "signature.target": "float",
     "coc.preferences.hoverTarget": "float"
}
